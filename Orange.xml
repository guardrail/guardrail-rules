<!-- Although shop.orange.co.uk supports ssl but on some page it keeps
falling back to http -->

<ruleset name="Orange">
  <target host="orange.co.uk" />
  <target host="www.orange.co.uk" />
  <target host="www.orange.co.il" />
  <target host="orange.co.il" />

  <rule from="^http://(www\.)?orange\.co\.uk/" to="https://www.orange.co.uk/"/>
  <rule from="^http://(www\.)?orange\.co\.il/" to="https://www.orange.co.il/"/>
</ruleset>
