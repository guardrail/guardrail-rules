<ruleset name="Airtricity">
  <target host="www.airtricity.com" />
  <target host="airtricity.com" />
  <target host="*.airtricity.com" />

  <rule from="^http://airtricity\.com/" to="https://www.airtricity.com/"/>
  <rule from="^http://([^/:@]*)\.airtricity\.com/" to="https://$1.airtricity.com/"/>
</ruleset>
