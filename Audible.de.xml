<ruleset name="Audible.de">
  <target host="audible.de"/>
  <target host="*.audible.de"/>

  <securecookie host="^(.*\.)?audible\.de$" name=".*" />

  <rule from="^http://(www\.)?audible\.de/" to="https://www.audible.de/"/>
  <rule from="^http://audible\.de/" to="https://www.audible.de/"/>
</ruleset>
