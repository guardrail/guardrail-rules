<ruleset name="MySQL">
  <target host="mysql.com" />
  <target host="*.mysql.com" />
  <exclusion pattern="^http://dev\.mysql\.com/" />
  <exclusion pattern="^http://bugs\.mysql\.com/" />

  <rule from="^http://mysql\.com/" to="https://www.mysql.com/"/>
  <rule from="^http://([^/:@]*)\.mysql\.com/" to="https://$1.mysql.com/"/>
</ruleset>
