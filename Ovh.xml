<ruleset name="OVH">
  <target host="ovh.com"/>
  <target host="www.ovh.com"/>
  <target host="ovh.co.uk"/>
  <target host="www.ovh.co.uk"/>
  
  <securecookie host="^(.*\.)?ovh.com$" name=".*" />
  <securecookie host="^(.*\.)?ovh.co.uk$" name=".*" />
  
  <rule from="^http://(www\.)?ovh\.com/" to="https://www.ovh.com/"/>
  <rule from="^http://(www\.)?ovh\.co\.uk/" to="https://www.ovh.co.uk/"/>
</ruleset>
